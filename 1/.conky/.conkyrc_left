maximum_width 1000
draw_shades no
draw_outline no
draw_borders no
draw_graph_borders no
default_color grey
default_shade_color red
default_outline_color green
alignment top_left
gap_x 14
gap_y 48
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale yes




